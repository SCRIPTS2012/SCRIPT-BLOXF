-- VOID_HUB - UNIVERSAL EXPLOIT HUB 2025
-- HOẠT ĐỘNG TRÊN MỌI GAME ROBLOX (MENU, LOBBY, OBBY, FPS, TYCOON, v.v.)

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera

-- === TRẠNG THÁI ===
local GUI_OPEN = false
local INF_JUMP = false
local FLYING = false
local NOCLIP = false
local AIRWALK = false
local GOD_MODE = false
local TPWALK_GUI = nil
local FLY_GUI = nil
local AIMBOT_PLAYER = false
local AIMBOT_NPC = false
local FOV_PLAYER = 100
local FOV_NPC = 150
local FOV_CIRCLE_PLAYER = nil
local FOV_CIRCLE_NPC = nil

-- === ESP TRẠNG THÁI ===
local ESP_HOLOGRAM = false
local ESP_HIGHLIGHTS = false
local ESP_NAME = false
local ESP_DISTANCE = false
local ESP_BOX = false

-- === AUTOWIN & FIXLAG ===
local AUTOWIN_ENABLED = false
local FIXLAG_ENABLED = false

local ESP_FOLDER = Instance.new("Folder")
ESP_FOLDER.Name = "VOID_ESP"
ESP_FOLDER.Parent = Workspace

-- Tạo GUI chính
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "VOID_HUB_GUI"
ScreenGui.Parent = game:GetService("CoreGui")
ScreenGui.ResetOnSpawn = false

-- ================== HIỆU ỨNG GLITCH INTRO ==================
local IntroGui = Instance.new("ScreenGui")
IntroGui.Name = "VOID_INTRO"
IntroGui.Parent = game:GetService("CoreGui")
IntroGui.ResetOnSpawn = false

local GlitchFrame = Instance.new("Frame")
GlitchFrame.Size = UDim2.new(1, 0, 1, 0)
GlitchFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
GlitchFrame.Parent = IntroGui

local glitchOverlay = Instance.new("ImageLabel")
glitchOverlay.Size = UDim2.new(1, 0, 1, 0)
glitchOverlay.BackgroundTransparency = 1
glitchOverlay.Image = "rbxassetid://215384789"
glitchOverlay.ImageTransparency = 0.7
hebOverlay.Parent = GlitchFrame

local lines = {}
for i = 1, 20 do
    local line = Instance.new("Frame")
    line.Size = UDim2.new(1, 0, 0, 2)
    line.Position = UDim2.new(0, 0, 0, math.random(0, 1000))
    line.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    line.BorderSizePixel = 0
    line.Parent = GlitchFrame
    table.insert(lines, line)
end

local voidText = Instance.new("TextLabel")
voidText.Size = UDim2.new(0, 700, 0, 120)
voidText.Position = UDim2.new(0.5, -350, 0.5, -60)
voidText.BackgroundTransparency = 1
voidText.Text = "VOID_HUB"
voidText.TextColor3 = Color3.fromRGB(0, 255, 0)
voidText.Font = Enum.Font.Code
voidText.TextSize = 60
voidText.TextStrokeTransparency = 0.5
voidText.TextStrokeColor3 = Color3.fromRGB(0, 100, 0)
voidText.Parent = GlitchFrame

spawn(function()
    local original = "VOID_HUB"
    for i = 1, 30 do
        local scrambled = ""
        for j = 1, #original do
            if math.random() < 0.7 then
                scrambled = scrambled .. string.char(math.random(33, 126))
            else
                scrambled = scrambled .. original:sub(j,j)
            end
        end
        voidText.Text = scrambled
        wait(0.05)
    end
    voidText.Text = original
end)

local columns = {}
for i = 1, math.floor((Camera.ViewportSize.X or 1920) / 18) do
    local col = {x = i * 18, y = math.random(-500, 0), speed = math.random(3, 8), length = math.random(6, 18), chars = {}}
    for j = 1, col.length do
        local char = Instance.new("TextLabel")
        char.Size = UDim2.new(0, 16, 0, 20)
        char.BackgroundTransparency = 1
        char.Text = string.char(math.random(33, 126))
        char.TextColor3 = Color3.fromRGB(0, 255, 0)
        char.Font = Enum.Font.Code
        char.TextSize = 14
        char.Parent = GlitchFrame
        table.insert(col.chars, char)
    end
    table.insert(columns, col)
end

spawn(function()
    local start = tick()
    while tick() - start < 5 do
        for _, col in pairs(columns) do
            col.y = col.y + col.speed
            if col.y > (Camera.ViewportSize.Y or 1080) + 100 then col.y = -100 end
            for i, char in pairs(col.chars) do
                local offset = (i - 1) * 20
                char.Position = UDim2.new(0, col.x, 0, col.y - offset)
                char.TextTransparency = i == 1 and 0 or (i / col.length * 0.8)
                if math.random() < 0.1 then char.Text = string.char(math.random(33, 126)) end
            end
        end
        for _, line in pairs(lines) do
            line.Position = UDim2.new(0, 0, 0, math.random(0, (Camera.ViewportSize.Y or 1080)))
            line.Visible = math.random() < 0.3
        end
        glitchOverlay.ImageTransparency = 0.6 + math.random() * 0.2
        RunService.Heartbeat:Wait()
    end
    IntroGui:Destroy()
end)

print("VOID_HUB UNIVERSAL - HOẠT ĐỘNG TRÊN MỌI GAME!")

