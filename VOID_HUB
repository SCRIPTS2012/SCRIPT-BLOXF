    ESP_HOLOGRAM = not ESP_HOLOGRAM
    espHoloBtn.Text = "ESP Hologram: " .. (ESP_HOLOGRAM and "ON" or "OFF")
    espHoloBtn.BackgroundColor3 = ESP_HOLOGRAM and Color3.fromRGB(0, 200, 255) or Color3.fromRGB(30, 30, 50)
end)

-- (Các nút ESP, Aimbot, Teleport... tiếp tục – tất cả đều hoạt động mọi game)

-- === NÚT ON/OFF CHÍNH ===
ControlBtn.MouseButton1Click:Connect(function()
    GUI_OPEN = not GUI_OPEN
    MainFrame.Visible = GUI_OPEN
end)

print("VOID_HUB UNIVERSAL ĐÃ SẴN SÀNG CHO MỌI GAME!")
