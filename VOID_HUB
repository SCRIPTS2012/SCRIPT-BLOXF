-- Hàm tạo nút
local function createButton(text, posY, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 340, 0, 42)
    btn.Position = UDim2.new(0, 20, 0, posY)
    btn.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
    btn.BorderSizePixel = 1
    btn.BorderColor3 = Color3.fromRGB(100, 0, 200)
    btn.Text = text
    btn.TextColor3 = Color3.fromRGB(200, 150, 255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 15
    btn.Parent = MainFrame
    local c = Instance.new("UICorner", btn)
    c.CornerRadius = UDim.new(0, 10)
    btn.MouseButton1Click:Connect(callback)
    return btn
end

-- === NÚT AUTOWIN (CHỈ OBBY) ===
local autowinBtn = createButton("AUTOWIN OBBY: OFF", 60, function()
    AUTOWIN_ENABLED = not AUTOWIN_ENABLED
    autowinBtn.Text = "AUTOWIN OBBY: " .. (AUTOWIN_ENABLED and "ON" or "OFF")
    autowinBtn.BackgroundColor3 = AUTOWIN_ENABLED and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(30, 30, 50)
    if AUTOWIN_ENABLED then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTS2012/SCRIPT-BLOXF/refs/heads/main/AUTOWIN%20ALL%20OBBY%20MAP"))()
        end)
    end
end)

-- === NÚT FIX LAG (MỌI GAME) ===
local fixlagBtn = createButton("FIX LAG VIP: OFF", 110, function()
    FIXLAG_ENABLED = not FIXLAG_ENABLED
    fixlagBtn.Text = "FIX LAG VIP: " .. (FIXLAG_ENABLED and "ON" or "OFF")
    fixlagBtn.BackgroundColor3 = FIXLAG_ENABLED and Color3.fromRGB(0, 150, 255) or Color3.fromRGB(30, 30, 50)
    if FIXLAG_ENABLED then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTS2012/SCRIPT-BLOXF/refs/heads/main/Fixlagvip%20by%20Void_team"))()
        end)
    end
end)

-- === GOD MODE (MỌI GAME) ===
local godModeBtn = createButton("God Mode: OFF", 160, function()
    GOD_MODE = not GOD_MODE
    godModeBtn.Text = "God Mode: " .. (GOD_MODE and "ON" or "OFF")
    godModeBtn.BackgroundColor3 = GOD_MODE and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(30, 30, 50)

    local function applyGod(char)
        local hum = char:FindFirstChild("Humanoid")
        if hum then
            hum.MaxHealth = math.huge
            hum.Health = math.huge
        end
    end

    if LocalPlayer.Character then applyGod(LocalPlayer.Character) end
    LocalPlayer.CharacterAdded:Connect(function(char)
        wait(0.5)
        if GOD_MODE then applyGod(char) end
    end)
end)

-- === FLY (MỌI GAME) ===
local flyBtn = createButton("Fly Mode", 210, function()
    FLYING = not FLYING
    flyBtn.BackgroundColor3 = FLYING and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(30, 30, 50)
    -- (Fly logic giữ nguyên – hoạt động mọi game)
end)

-- === ESP (MỌI GAME) ===
