-- VOID_HUB - UNIVERSAL EXPLOIT HUB 2025
-- HOẠT ĐỘNG MỌI NƠI: MENU, LOBBY, IN-GAME | KHÔNG LỖI MÀN HÌNH ĐEN

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer
local Camera = Workspace.CurrentCamera

-- === TRẠNG THÁI ===
local GUI_OPEN = false
local INF_JUMP = false
local FLYING = false
local NOCLIP = false
local AIRWALK = false
local GOD_MODE = false
local TPWALK_GUI = nil
local FLY_GUI = nil
local AIMBOT_PLAYER = false
local AIMBOT_NPC = false
local FOV_PLAYER = 100
local FOV_NPC = 150

-- === ESP TRẠNG THÁI ===
local ESP_HOLOGRAM = false
local ESP_HIGHLIGHTS = false
local ESP_NAME = false
local ESP_DISTANCE = false
local ESP_BOX = false

-- === AUTOWIN & FIXLAG ===
local AUTOWIN_ENABLED = false
local FIXLAG_ENABLED = false

local ESP_FOLDER = Instance.new("Folder")
ESP_FOLDER.Name = "VOID_ESP"
ESP_FOLDER.Parent = Workspace

-- ================== GUI CHÍNH – HIỆN NGAY LẬP TỨC ==================
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "VOID_HUB_GUI"
ScreenGui.Parent = game:GetService("CoreGui")
ScreenGui.ResetOnSpawn = false

-- Nút mở GUI
local ControlBtn = Instance.new("TextButton")
ControlBtn.Size = UDim2.new(0, 140, 0, 50)
ControlBtn.Position = UDim2.new(0, 10, 0, 10)
ControlBtn.BackgroundColor3 = Color3.fromRGB(10, 10, 20)
ControlBtn.BorderSizePixel = 2
ControlBtn.BorderColor3 = Color3.fromRGB(100, 0, 255)
ControlBtn.Text = "VOID_HUB"
ControlBtn.TextColor3 = Color3.fromRGB(200, 0, 255)
ControlBtn.Font = Enum.Font.GothamBlack
ControlBtn.TextSize = 20
ControlBtn.Parent = ScreenGui

local corner = Instance.new("UICorner", ControlBtn)
corner.CornerRadius = UDim.new(0, 14)

local gradient = Instance.new("UIGradient")
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(50, 0, 100)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(150, 0, 255))
}
gradient.Rotation = 45
gradient.Parent = ControlBtn

-- Main Frame
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 380, 0, 900)
MainFrame.Position = UDim2.new(0, 160, 0, 10)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
MainFrame.BorderSizePixel = 2
MainFrame.BorderColor3 = Color3.fromRGB(120, 0, 255)
MainFrame.Visible = false
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

local mfCorner = Instance.new("UICorner", MainFrame)
mfCorner.CornerRadius = UDim.new(0, 16)

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 50)
Title.BackgroundColor3 = Color3.fromRGB(20, 0, 60)
Title.Text = "VOID_HUB - UNIVERSAL"
Title.TextColor3 = Color3.fromRGB(200, 100, 255)
Title.Font = Enum.Font.GothamBlack
Title.TextSize = 20
Title.Parent = MainFrame

-- Hàm tạo nút
local function createButton(text, posY, callback)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 340, 0, 42)
    btn.Position = UDim2.new(0, 20, 0, posY)
    btn.BackgroundColor3 = Color3.fromRGB(30, 30, 50)
    btn.BorderSizePixel = 1
    btn.BorderColor3 = Color3.fromRGB(100, 0, 200)
    btn.Text = text
    btn.TextColor3 = Color3.fromRGB(200, 150, 255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 15
    btn.Parent = MainFrame
    local c = Instance.new("UICorner", btn)
    c.CornerRadius = UDim.new(0, 10)
    btn.MouseButton1Click:Connect(callback)
    return btn
end

-- === NÚT AUTOWIN OBBY ===
local autowinBtn = createButton("AUTOWIN OBBY: OFF", 60, function()
    AUTOWIN_ENABLED = not AUTOWIN_ENABLED
    autowinBtn.Text = "AUTOWIN OBBY: " .. (AUTOWIN_ENABLED and "ON" or "OFF")
    autowinBtn.BackgroundColor3 = AUTOWIN_ENABLED and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(30, 30, 50)
    if AUTOWIN_ENABLED then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTS2012/SCRIPT-BLOXF/refs/heads/main/AUTOWIN%20ALL%20OBBY%20MAP"))()
        end)
    end
end)

-- === NÚT FIX LAG VIP ===
local fixlagBtn = createButton("FIX LAG VIP: OFF", 110, function()
    FIXLAG_ENABLED = not FIXLAG_ENABLED
    fixlagBtn.Text = "FIX LAG VIP: " .. (FIXLAG_ENABLED and "ON" or "OFF")
    fixlagBtn.BackgroundColor3 = FIXLAG_ENABLED and Color3.fromRGB(0, 150, 255) or Color3.fromRGB(30, 30, 50)
    if FIXLAG_ENABLED then
        pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTS2012/SCRIPT-BLOXF/refs/heads/main/Fixlagvip%20by%20Void_team"))()
        end)
    end
end)

-- === GOD MODE ===
local godModeBtn = createButton("God Mode: OFF", 160, function()
    GOD_MODE = not GOD_MODE
    godModeBtn.Text = "God Mode: " .. (GOD_MODE and "ON" or "OFF")
    godModeBtn.BackgroundColor3 = GOD_MODE and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(30, 30, 50)

    local function applyGod(char)
        local hum = char:FindFirstChild("Humanoid")
        if hum then
            hum.MaxHealth = math.huge
            hum.Health = math.huge
        end
    end

    if LocalPlayer.Character then applyGod(LocalPlayer.Character) end
    LocalPlayer.CharacterAdded:Connect(function(char)
        wait(0.5)
        if GOD_MODE then applyGod(char) end
    end)
end)

-- === FLY MODE ===
local flyBtn = createButton("Fly Mode: OFF", 210, function()
    FLYING = not FLYING
    flyBtn.Text = "Fly Mode: " .. (FLYING and "ON" or "OFF")
    flyBtn.BackgroundColor3 = FLYING and Color3.fromRGB(0, 200, 255) or Color3.fromRGB(30, 30, 50)

    if FLYING then
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
        bodyVelocity.Parent = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        RunService.Stepped:Connect(function()
            if FLYING and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local hrp = LocalPlayer.Character.HumanoidRootPart
                local cam = Workspace.CurrentCamera
                local move = Vector3.new(
                    (UserInputService:IsKeyDown(Enum.KeyCode.D) and 1 or 0) - (UserInputService:IsKeyDown(Enum.KeyCode.A) and 1 or 0),
                    (UserInputService:IsKeyDown(Enum.KeyCode.Space) and 1 or 0) - (UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) and 1 or 0),
                    (UserInputService:IsKeyDown(Enum.KeyCode.W) and 1 or 0) - (UserInputService:IsKeyDown(Enum.KeyCode.S) and 1 or 0)
                )
                hrp.Velocity = (cam.CFrame:VectorToWorldSpace(move) * 200)
            end
        end)
    end
end)

-- === ESP HOLOGRAM (UNIVERSAL) ===
local espHoloBtn = createButton("ESP Hologram: OFF", 260, function()
    ESP_HOLOGRAM = not ESP_HOLOGRAM
    espHoloBtn.Text = "ESP Hologram: " .. (ESP_HOLOGRAM and "ON" or "OFF")
    espHoloBtn.BackgroundColor3 = ESP_HOLOGRAM and Color3.fromRGB(0, 200, 255) or Color3.fromRGB(30, 30, 50)
end)

-- === TELEPORT TO PLAYERS (MENU + IN-GAME) ===
local PlayerListFrame = Instance.new("ScrollingFrame")
PlayerListFrame.Size = UDim2.new(0, 340, 0, 120)
PlayerListFrame.Position = UDim2.new(0, 20, 0, 310)
PlayerListFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
PlayerListFrame.BorderSizePixel = 1
PlayerListFrame.BorderColor3 = Color3.fromRGB(100, 0, 200)
PlayerListFrame.ScrollBarThickness = 6
PlayerListFrame.Parent = MainFrame

local listTitle = Instance.new("TextLabel")
listTitle.Size = UDim2.new(1, 0, 0, 30)
listTitle.BackgroundTransparency = 1
listTitle.Text = "TELEPORT TO PLAYERS"
listTitle.TextColor3 = Color3.fromRGB(200, 150, 255)
listTitle.Font = Enum.Font.GothamBold
listTitle.TextSize = 14
listTitle.Parent = PlayerListFrame

local playerButtons = {}
local function updatePlayerList()
    for _, btn in pairs(playerButtons) do btn:Destroy() end
    playerButtons = {}

    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, -10, 0, 35)
            btn.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
            btn.Text = plr.Name
            btn.TextColor3 = Color3.fromRGB(200, 200, 255)
            btn.Font = Enum.Font.Gotham
            btn.TextSize = 13
            btn.Parent = PlayerListFrame
            local c = Instance.new("UICorner", btn)
            c.CornerRadius = UDim.new(0, 8)

            btn.MouseButton1Click:Connect(function()
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local target = plr.Character
                    if target and target:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = target.HumanoidRootPart.CFrame + Vector3.new(0, 3, 0)
                    end
                end
            end)
            table.insert(playerButtons, btn)
        end
    end
    PlayerListFrame.CanvasSize = UDim2.new(0, 0, 0, #playerButtons * 39 + 34)
end

Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoving:Connect(updatePlayerList)

-- === MỞ GUI ===
ControlBtn.MouseButton1Click:Connect(function()
    GUI_OPEN = not GUI_OPEN
    MainFrame.Visible = GUI_OPEN
    ControlBtn.Text = GUI_OPEN and "CLOSE" or "VOID_HUB"
    if GUI_OPEN then updatePlayerList() end
end)

-- === GLITCH INTRO CHỈ CHẠY KHI VÀO GAME THẬT SỰ ===
local introRan = false
LocalPlayer.CharacterAdded:Connect(function()
    if introRan then return end
    introRan = true

    local IntroGui = Instance.new("ScreenGui")
    IntroGui.Parent = game:GetService("CoreGui")
    IntroGui.ResetOnSpawn = false

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1, 0, 1, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Frame.Parent = IntroGui

    local Text = Instance.new("TextLabel")
    Text.Size = UDim2.new(0, 700, 0, 120)
    Text.Position = UDim2.new(0.5, -350, 0.5, -60)
    Text.BackgroundTransparency = 1
    Text.Text = "VOID_HUB"
    Text.TextColor3 = Color3.fromRGB(0, 255, 0)
    Text.Font = Enum.Font.Code
    Text.TextSize = 60
    Text.Parent = Frame

    spawn(function()
        wait(3)
        IntroGui:Destroy()
    end)
end)

print("VOID_HUB UNIVERSAL ĐÃ SẴN SÀNG! Bấm nút VOID_HUB để mở.")
