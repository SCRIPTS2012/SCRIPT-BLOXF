Submit.MouseButton1Click:Connect(function()
    if Input.Text == validKey then
        keyEntered = true
        KeyWindow:Destroy()
        print("CHÚC MỪNG EM! 30/4/1975 VTHUB ĐANG MỞ...")
    else
        Input.Text = ""
        Input.PlaceholderText = "phải gì phải chịu đừng có kêu! Thử lại đi ku (Voidteam_hub)."
    end
end)

while not keyEntered do
    wait(0.1)
end

