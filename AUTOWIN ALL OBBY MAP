-- ===== Logic t√¨m ƒë√≠ch =====
local FINISH_KEYWORDS = {
    "finish","goal","end","exit","win",
    "finishline","endpoint","victory","goalpart","finishpart","exitpart"
}

local function isFinishName(name)
    if not name then return false end
    local n = tostring(name):lower()
    for _,k in ipairs(FINISH_KEYWORDS) do
        if n:find(k) then return true end
    end
    return false
end

local function findFinishParts()
    local results = {}
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            if isFinishName(obj.Name) or (obj.Parent and isFinishName(obj.Parent.Name)) then
                table.insert(results, obj)
            end
        end
    end
    return results
end

local function getFarthestFinish()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local parts = findFinishParts()
    if #parts == 0 then return nil end
    local farthest, farDist = nil, -math.huge
    for _,p in ipairs(parts) do
        local ok, pos = pcall(function() return p.Position end)
        if ok and pos then
            local d = (hrp.Position - pos).Magnitude
            if d > farDist then
                farDist = d
                farthest = p
            end
        end
    end
    return farthest
end

local function teleportTo(part)
    if not part then return end
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local success = false
    if typeof(firetouchinterest) == "function" then
        pcall(function()
            firetouchinterest(hrp, part, 0)
            firetouchinterest(hrp, part, 1)
            success = true
        end)
    end
    if not success then
        pcall(function()
            hrp.CFrame = part.CFrame + Vector3.new(0,3,0)
        end)
    end
end

-- ===== Overlay t·∫°o hi·ªáu ·ª©ng =====
local function showOverlay(msg)
    local overlay = Instance.new("Frame", gui)
    overlay.Size = UDim2.new(1,0,1,0)
    overlay.BackgroundColor3 = Color3.fromRGB(0,0,0)
    overlay.BackgroundTransparency = 0.4

    -- credit
    local credit = Instance.new("TextLabel", overlay)
    credit.Size = UDim2.new(1,0,0,40)
    credit.Position = UDim2.new(0,0,0,0)
    credit.BackgroundTransparency = 1
    credit.Text = "by üëæVoid_teamüáªüá≥"
    credit.Font = Enum.Font.GothamBold
    credit.TextSize = 24
    credit.TextColor3 = Color3.fromRGB(0,255,0)
    credit.TextStrokeTransparency = 0.2

    -- text ch√≠nh
    local text = Instance.new("TextLabel", overlay)
    text.Size = UDim2.new(1,0,0.2,0)
    text.Position = UDim2.new(0,0,0.4,0)
    text.BackgroundTransparency = 1
    text.Text = msg
    text.Font = Enum.Font.GothamBlack
    text.TextSize = 32
    text.TextColor3 = Color3.fromRGB(255,0,255)
    text.TextStrokeTransparency = 0.2

    local uiStroke = Instance.new("UIStroke", text)
    uiStroke.Thickness = 2
    uiStroke.Color = Color3.fromRGB(255,255,255)
    uiStroke.Transparency = 0.2

    return overlay, text, uiStroke
end

local function pulseEffect(label, stroke, times)
    for i=1, times do
        for t=0,1,0.05 do
            label.TextColor3 = Color3.fromHSV(0.83, 1, t)
            stroke.Transparency = 0.5 - (t/2)
            task.wait(0.05)
        end
        for t=1,0,-0.05 do
            label.TextColor3 = Color3.fromHSV(0.83, 1, t)
            stroke.Transparency = 0.5 - (t/2)
            task.wait(0.05)
        end
    end
end

-- ===== Khi b·∫•m n√∫t =====
button.MouseButton1Click:Connect(function()
    local overlay, text, stroke = showOverlay("‚ú® H√£y ƒë·ª£i m·ªôt t√Ω... Void_GUIüëæ ƒëang t√¨m ki·∫øm ‚ú®")
    task.wait(2)
    local target = getFarthestFinish()
    if target then
        teleportTo(target)
        text.Text = "üáªüá≥ C·∫£m ∆°n ƒë√£ s·ª≠ d·ª•ng Void_GUIüëæ :)"
        pulseEffect(text, stroke, 3)
    else
        text.Text = "‚ùå Kh√¥ng t√¨m th·∫•y ƒë√≠ch trong map n√†y!"
    end
    task.wait(4)
    overlay:Destroy()
end)
