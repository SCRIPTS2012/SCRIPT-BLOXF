local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local VirtualUser = game:GetService("VirtualUser")

-- GUI
local Window = Rayfield:CreateWindow({
    Name = "‚öîÔ∏è SDowHub Dream Anti-Ban | Blox Fruits",
    LoadingTitle = "SDowHub Dream Loading...",
    LoadingSubtitle = "by ShadowVN Kaitou",
    ConfigurationSaving = { Enabled = true, FolderName = "SDowHub", FileName = "BloxFruits" },
    KeySystem = true,
    KeySettings = { Title = "SDowHub Key", Subtitle = "Nh·∫≠p key", Note = "Key = sdowhub", FileName = "SDowHubKey", SaveKey = true, GrabKeyFromSite = false, Key = {"sdowhub"} }
})

-- Tabs
local Main = Window:CreateTab("‚öîÔ∏è Auto Farm", 4483362458)
local Teleport = Window:CreateTab("üåç Teleport", 4483362458)
local Misc = Window:CreateTab("‚öôÔ∏è Misc", 4483362458)

-- Variables
local AutoFarmEnabled, AutoQuestEnabled, AutoSkillEnabled, AutoBossEnabled, AutoDFEnabled, AutoLevelEnabled = false,false,false,false,false,false
local AimEnabled, AimRadius, TargetPlayer = false,50,nil

-- SEA DATA
local QuestData = {
    ["Sea1"] = { NPC=Vector3.new(100,0,100), Quests={{Mob="Bandit [Lv. 5]",Amount=10},{Mob="Monkey [Lv. 14]",Amount=12}}, Boss={"Bandit Leader [Lv. 50]"}, Mobs={"Bandit [Lv. 5]","Monkey [Lv. 14]"} },
    ["Sea2"] = { NPC=Vector3.new(-500,0,200), Quests={{Mob="Pirate [Lv. 20]",Amount=15},{Mob="Brute [Lv. 35]",Amount=10}}, Boss={"Pirate Captain [Lv. 100]"}, Mobs={"Pirate [Lv. 20]","Brute [Lv. 35]"} },
    ["Sea3"] = { NPC=Vector3.new(200,0,-300), Quests={{Mob="Marine Captain [Lv. 40]",Amount=15},{Mob="Fishman Warrior [Lv. 50]",Amount=12}}, Boss={"Marine Admiral [Lv. 200]"}, Mobs={"Marine Captain [Lv. 40]","Fishman Warrior [Lv. 50]"} }
}

local function getSea(level)
    if level < 50 then return "Sea1"
    elseif level < 150 then return "Sea2"
    else return "Sea3"
end

-- Helper functions
local function getNearestEnemy(names)
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return nil end
    local nearest,minDist = nil,math.huge
    for _,v in pairs(Workspace.Enemies:GetChildren()) do
        if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and table.find(names,v.Name) then
            local dist = (v.HumanoidRootPart.Position - hrp.Position).Magnitude
            if dist<minDist then nearest=v minDist=dist end
        end
    end
    return nearest
end

local function getNearestDF()
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return nil end
    local nearest,minDist = nil,math.huge
    for _,v in pairs(Workspace:GetChildren()) do
        if v:IsA("Tool") and v.Name:find("Fruit") then
            local dist = (v.Position - hrp.Position).Magnitude
            if dist<minDist then nearest=v minDist=dist end
        end
    end
    return nearest
end

local function TweenTo(hrp,pos)
    local distance = (hrp.Position-pos).Magnitude
    local tweenInfo = TweenInfo.new(distance/150,Enum.EasingStyle.Linear,Enum.EasingDirection.Out)
    local tween = TweenService:Create(hrp,tweenInfo,{CFrame=CFrame.new(pos+Vector3.new(0,5,0))})
    tween:Play()
    tween.Completed:Wait()
end

-- Auto Farm Loop
local function AutoFarmLoop()
    spawn(function()
        while AutoFarmEnabled do task.wait(math.random(0.2,0.5))
            pcall(function()
                local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                local level = LocalPlayer.Data.Level.Value
                if level >= 2660 then AutoFarmEnabled=false break end
                local Sea = getSea(level)
                local data = QuestData[Sea]

                if AutoQuestEnabled then
                    TweenTo(hrp,data.NPC)
                    ReplicatedStorage.Remotes.CommF_:InvokeServer("StartQuest")
                    task.wait(math.random(1,2))
                end

                if AutoSkillEnabled then
                    pcall(function()
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("UseSkill")
                    end)
                end

                if AutoDFEnabled then
                    local df = getNearestDF()
                    if df then TweenTo(hrp,df.Position) task.wait(0.2) df.Parent=LocalPlayer.Backpack end
                end

                if AutoBossEnabled then
                    local bossTarget = getNearestEnemy(data.Boss)
                    if bossTarget and bossTarget:FindFirstChild("HumanoidRootPart") then
                        TweenTo(hrp,bossTarget.HumanoidRootPart.Position)
                        pcall(function()
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("UseSkill")
                        end)
                        task.wait(math.random(0.3,0.7))
                        return
                    end
                end

                local target = getNearestEnemy(data.Mobs)
                if target and target:FindFirstChild("HumanoidRootPart") then
                    TweenTo(hrp,target.HumanoidRootPart.Position)
                    task.wait(math.random(0.1,0.3))
                    VirtualUser:ClickButton1(Vector2.new())
                    task.wait(math.random(0.2,0.5))
                end

                if AutoLevelEnabled then
                    pcall(function()
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("UpgradeSkill")
                        ReplicatedStorage.Remotes.CommF_:InvokeServer("UpgradeBuso")
                    end)
                end
            end)
        end
    end)
end

-- GUI Toggles
Main:CreateToggle({ Name="Auto Farm Sea 1-3", CurrentValue=false, Flag="AutoFarm", Callback=function(V) AutoFarmEnabled=V AutoFarmLoop() end })
Main:CreateToggle({ Name="Auto Quest", CurrentValue=false, Flag="AutoQuest", Callback=function(V) AutoQuestEnabled=V end })
Main:CreateToggle({ Name="Auto Skill & Buso", CurrentValue=false, Flag="AutoSkill", Callback=function(V) AutoSkillEnabled=V end })
Main:CreateToggle({ Name="Auto Boss Sea 1-3", CurrentValue=false, Flag="AutoBoss", Callback=function(V) AutoBossEnabled=V end })
Main:CreateToggle({ Name="Auto Devil Fruit", CurrentValue=false, Flag="AutoDF", Callback=function(V) AutoDFEnabled=V end })
Main:CreateToggle({ Name="Auto Level Up Skill & Buso", CurrentValue=false, Flag="AutoLevel", Callback=function(V) AutoLevelEnabled=V end })

Main:CreateButton({ Name="B·∫≠t Buso Haki", Callback=function() pcall(function() ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso") end) end })

-- Teleport Dropdown
Teleport:CreateDropdown({ Name="D·ªãch chuy·ªÉn ƒë·∫£o", Options={"Starter Island","Marine","Jungle","Sky Island"}, CurrentOption={"Starter Island"}, Flag="TPIsland", Callback=function(Option)
    local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    local positions = {["Starter Island"]=Vector3.new(1100,15,1100),["Marine"]=Vector3.new(-2500,20,2200),["Jungle"]=Vector3.new(-1500,20,1000),["Sky Island"]=Vector3.new(-5000,500,-1000)}
    hrp.CFrame = CFrame.new(positions[Option])
end})

-- Anti AFK
Misc:CreateButton({ Name="Anti AFK", Callback=function()
    LocalPlayer.Idled:Connect(function()
        VirtualUser:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        task.wait(1)
        VirtualUser:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end })

-- Aim Player PvP
Main:CreateToggle({ Name = "Aim Player PvP", CurrentValue = false, Flag = "AimPlayer", Callback = function(Value) AimEnabled = Value end })
Main:CreateSlider({ Name = "Aim Radius", Range = {10,100}, Increment = 1, Suffix = " studs", CurrentValue = 50, Flag = "AimRadiusSlider", Callback = function(Value) AimRadius = Value end })

RunService.RenderStepped:Connect(function()
    if AimEnabled then
        local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        local nearestPlayer,minDist = nil,math.huge
        for _,plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Humanoid") and plr.Character.Humanoid.Health>0 then
                local dist = (plr.Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                if dist < minDist and dist <= AimRadius then
                    minDist = dist
                    nearestPlayer = plr
                end
            end
        end
        TargetPlayer = nearestPlayer
        if TargetPlayer and TargetPlayer.Character and TargetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            hrp.CFrame = CFrame.new(hrp.Position, TargetPlayer.Character.HumanoidRootPart.Position)
            pcall(function()
                ReplicatedStorage.Remotes.CommF_:InvokeServer("UseSkill")
                VirtualUser:ClickButton1(Vector2.new())
            end)
        end
    end
end)

Rayfield:LoadConfiguration()
print("‚öîÔ∏è SDowHub Dream Anti-Ban Loaded | By ShadowVN Kaitou")
